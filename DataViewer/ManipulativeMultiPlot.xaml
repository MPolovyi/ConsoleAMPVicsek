<UserControl x:Class="DataViewer.ManipulativeMultiPlot"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Canvas Name="MyCanvas" x:FieldModifier="protected" Background="White">
        <Label Name="ActiveLabel" Canvas.Right="10" Canvas.Top="10" x:FieldModifier="private"></Label>
        <Slider Name="MySlider" Canvas.Right="10" Canvas.Bottom="10" Width="100" SmallChange="1" ValueChanged="MySlider_OnValueChanged"></Slider>
    </Canvas>
</UserControl>
