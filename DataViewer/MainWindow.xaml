<Window x:Class="DataViewer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="650" Width="850">
    <Canvas>
        <StackPanel Orientation="Horizontal" Height="25" Margin="15, 15, 0, 0">
            <Button x:Name="OpenButton" Width="80" Content="Open" Click="OpenButton_Click"/>
        </StackPanel>
        <StackPanel x:Name="ImgPanel" Orientation="Horizontal" Canvas.Left="25" Canvas.Top="50" Width="800" Height="500">
            <Path x:Name="MyPath"></Path>
            <Image x:Name="TimeFlowImage" Width="395"></Image>
            <Image x:Name="HeigthDependance" Width="395" Margin="10, 0, 0, 0"></Image>
        </StackPanel>
        <Button Width="80" Height="25" Canvas.Left="733" Canvas.Top="571">Close</Button>
    </Canvas>
</Window>
